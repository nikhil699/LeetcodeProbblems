class Solution(object):
    def missingNumber(self, nums):
        size = len(nums)
        totalSum = sum(nums) 
        # print(totalSum) 
        overallSum = size * (size + 1) // 2  

        return overallSum - totalSum


sol = Solution()
nums = [9,6,4,2,3,5,7,0,1]

print(sol.missingNumber(nums))